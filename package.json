{
  "name": "dds-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "yarn sass:build && remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "sass": "sass --watch app/:app/",
    "sass:build": "sass app/:app/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:deploy": "storybook build && wrangler deploy"
  },
  "dependencies": {
    "@icon-park/react": "^1.4.2",
    "@remix-run/cloudflare": "2.8.0",
    "@remix-run/cloudflare-pages": "2.8.0",
    "@remix-run/css-bundle": "2.8.0",
    "@remix-run/react": "2.8.0",
    "clsx": "^2.1.0",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.18",
    "@cloudflare/kv-asset-handler": "^0.3.1",
    "@cloudflare/workers-types": "^4.20240222.0",
    "@remix-run/dev": "2.8.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/react-webpack5": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "sass": "^1.71.1",
    "storybook": "^8.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "wrangler": "^3.31.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.1.1"
}
